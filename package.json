{
  "name": "dependencies-version",
  "version": "1.0.0",
  "description": "List your runtime dependencies and their versions",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "watch:test": "npm t -- --watch",
    "test": "mocha src/index.test.js --compilers js:babel-core/register",
    "check-jshint": "jshint src/index.js",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsachis/dependencies-version.git"
  },
  "keywords": [
    "npm",
    "package",
    "dependencies",
    "modules"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Tsachi Shushan",
    "email": "tsachishushan@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsachis/dependencies-version/issues"
  },
  "homepage": "https://github.com/tsachis/dependencies-version#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "3.5.0",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "jshint": "2.9.4",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.0",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "semantic-release": "^6.3.2",
    "webpack": "1.14.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run check-jshint"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
